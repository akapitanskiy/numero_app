<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_90"
    tools:context=".ui.people.details.PythagoreanMatrixFragment"
    >

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/matrixTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/pythagorean_matrix"
        android:textSize="19sp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/birthDateMatrix"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/matrixTitle"
        app:layout_constraintStart_toStartOf="parent"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf1s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginTop="8dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@id/birthDateMatrix"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/txtOf2s"
        tools:text="111"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf2s"
        style="@style/Theme.AppTheme.MatrixItem"
        app:layout_constraintBottom_toBottomOf="@id/txtOf1s"
        app:layout_constraintStart_toEndOf="@id/txtOf1s"
        app:layout_constraintEnd_toStartOf="@id/txtOf3s"
        tools:text="2222"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf3s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@id/txtOf1s"
        app:layout_constraintStart_toEndOf="@id/txtOf2s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="3333"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf4s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf1s"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/txtOf2s"
        tools:text="44"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf5s"
        style="@style/Theme.AppTheme.MatrixItem"
        app:layout_constraintTop_toBottomOf="@id/txtOf1s"
        app:layout_constraintStart_toEndOf="@id/txtOf1s"
        app:layout_constraintEnd_toStartOf="@id/txtOf3s"
        tools:text="55"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf6s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf1s"
        app:layout_constraintStart_toEndOf="@id/txtOf2s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="6"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf7s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf4s"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/txtOf8s"
        tools:text="77"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf8s"
        style="@style/Theme.AppTheme.MatrixItem"
        app:layout_constraintTop_toBottomOf="@id/txtOf4s"
        app:layout_constraintStart_toEndOf="@id/txtOf7s"
        app:layout_constraintEnd_toStartOf="@id/txtOf9s"
        tools:text="88"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOf9s"
        style="@style/Theme.AppTheme.MatrixItem"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf4s"
        app:layout_constraintStart_toEndOf="@id/txtOf8s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="9"
        />



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo1s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginTop="24dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf7s"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="111"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo2s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo1s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo1s"
        tools:text="2222222"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo3s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo1s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo2s"
        tools:text="333333"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo4s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/mtrxTwo1s"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="444444"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo5s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo4s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo1s"
        tools:text="5555"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo6s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo4s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo2s"
        tools:text="6"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo7s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/mtrxTwo4s"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="777"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo8s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo7s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo1s"
        tools:text="8888"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrxTwo9s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrxTwo7s"
        app:layout_constraintStart_toEndOf="@id/mtrxTwo2s"
        tools:text="999"
        />



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_1s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_3s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_2s"
        tools:text="111"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_2s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_3s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_3s"
        tools:text="2222222"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_3s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toBottomOf="@id/txtOf7s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="333333"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_4s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_6s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_5s"
        tools:text="444444"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_5s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_6s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_6s"
        tools:text="5555"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_6s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/mtrx3_3s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="6"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_7s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_9s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_8s"
        tools:text="777"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_8s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        app:layout_constraintBottom_toBottomOf="@id/mtrx3_9s"
        app:layout_constraintEnd_toStartOf="@id/mtrx3_9s"
        tools:text="8888"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mtrx3_9s"
        style="@style/Theme.AppTheme.MatrixItemSmall"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/mtrx3_6s"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="999"
        />




    <androidx.constraintlayout.widget.Group
        android:id="@+id/groupMatrix2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="mtrxTwo1s, mtrxTwo2s, mtrxTwo3s, mtrxTwo4s, mtrxTwo5s, mtrxTwo6s, mtrxTwo7s, mtrxTwo8s, mtrxTwo9s"
        />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/groupMatrix3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="mtrx3_1s, mtrx3_2s, mtrx3_3s, mtrx3_4s, mtrx3_5s, mtrx3_6s, mtrx3_7s, mtrx3_8s, mtrx3_9s, "
        />




</androidx.constraintlayout.widget.ConstraintLayout>